# Database Configuration
DATABASE_URL=postgresql://qwen_user:your_password_here@postgres:5432/qwen_layered

# Storage Configuration
FILE_STORAGE_PATH=/storage
FILE_RETENTION_DAYS=7
MAX_FILE_SIZE_MB=50

# Model Configuration
MODEL_CACHE_DIR=/models
CUDA_VISIBLE_DEVICES=0

# Model Names (optional, defaults are provided)
# DECOMPOSITION_MODEL=Qwen/Qwen-Image-Layered
# EDITING_MODEL=Qwen/Qwen-Image-Edit-2509
# RMBG_MODEL=briaai/RMBG-2.0

# API Configuration
CORS_ORIGINS=http://localhost:3000,http://localhost
API_TITLE=Qwen Image Layered API
API_VERSION=1.0.0

# Redis Configuration (optional)
REDIS_URL=redis://redis:6379/0

# Application Configuration
ENVIRONMENT=development
DEBUG=false

# Default Inference Parameters
DEFAULT_RESOLUTION=640
MAX_INFERENCE_STEPS=50
MAX_LAYERS=10

# PostgreSQL Database (for docker-compose)
DB_USER=qwen_user
DB_PASSWORD=your_secure_password_here
